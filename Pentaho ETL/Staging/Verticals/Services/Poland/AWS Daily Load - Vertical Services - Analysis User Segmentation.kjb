    <entry>
      <name>SQL vert_services_fixly_buckets_segment</name>
      <description/>
      <type>SQL</type>
      <sql>truncate table odl_global_verticals.vert_services_fixly_buckets_segment&#x3b;&#xd;&#xa;&#xd;&#xa;insert into odl_global_verticals.vert_services_fixly_buckets_segment&#xd;&#xa;with ads AS&#xd;&#xa;&#x28;&#xd;&#xa;    SELECT &#x2a;&#xd;&#xa;    FROM rdl_triton_livesync.cee_olxpl_ads ads&#xd;&#xa;    WHERE ads.livesync_dbname &#x3d; &#x27;olxpl&#x27;&#xd;&#xa;          AND net_ad_counted &#x3d; 1&#xd;&#xa;          AND category_id IN &#x28;619, 131, 133, &#x2f;&#x2a;168,&#x2a;&#x2f; 179, 297, 300, 306, 314, 354, 356, 368, &#x2f;&#x2a;375,&#x2a;&#x2f; 522, 523, 625, 713, 741&#x29;&#xd;&#xa;          AND &#x28;trunc&#x28;created_at_first&#x29; &#x3e;&#x3d; CURRENT_DATE-180 OR status &#x3d; &#x27;active&#x27;&#x29;&#xd;&#xa;          AND NOT &#x28;category_id &#x3d; 131 AND &#x28;&#xd;&#xa;            upper&#x28;title&#x29; LIKE &#x27;&#x25;P__YCZK&#x25;&#x27; OR&#xd;&#xa;            upper&#x28;title&#x29; LIKE &#x27;&#x25;GOT_WKA&#x25;&#x27; OR&#xd;&#xa;            upper&#x28;title&#x29; LIKE &#x27;&#x25;PO_YCZANIE&#x25;&#x27; OR&#xd;&#xa;            upper&#x28;title&#x29; LIKE &#x27;&#x25;KREDYT&#x25;&#x27; OR&#xd;&#xa;            upper&#x28;title&#x29; LIKE &#x27;&#x25;P__YCZKI&#x25;&#x27;&#xd;&#xa;           &#x29;&#x29;&#xd;&#xa;&#x29;,&#xd;&#xa;users AS -- users who have posted in services category within last 6 months union ---&#x3e; users who have at least 1 active ad in services category&#xd;&#xa;&#x28;select user_id, email, name_pl&#xd;&#xa;  from ads&#xd;&#xa;    join rdl_triton_livesync.cee_olxpl_users usr&#xd;&#xa;      on ads.user_id&#x3d;usr.id&#xd;&#xa;    join rdl_triton_livesync.cee_olxpl_categories cat&#xd;&#xa;      on ads.category_id&#x3d;cat.id&#xd;&#xa;    group by user_id, email, name_pl&#xd;&#xa;&#x29;,&#xd;&#xa;-- users checked&#xd;&#xa;nnls as -- numbers of nnls posted in services category within last 6 months&#xd;&#xa;&#x28;&#xd;&#xa;      select a1.user_id,&#xd;&#xa;        cat.name_pl,&#xd;&#xa;        count&#x28;distinct a1.id&#x29; nnls&#xd;&#xa;      from ads a1&#xd;&#xa;        join rdl_triton_livesync.cee_olxpl_categories cat&#xd;&#xa;          on a1.category_id&#x3d;cat.id&#xd;&#xa;        join users a2&#xd;&#xa;          on a1.user_id&#x3d;a2.user_id&#xd;&#xa;      where a1.net_ad_counted&#x3d;1 and trunc&#x28;created_at_first&#x29; &#x3e;&#x3d; CURRENT_DATE-180&#xd;&#xa;      group by a1.user_id,cat.name_pl&#xd;&#xa;&#x29;,&#xd;&#xa;-- nnl checked&#xd;&#xa;active_ads as  --  number of active ads in services category&#xd;&#xa;&#x28;  select a1.user_id,&#xd;&#xa;     cat.name_pl,&#xd;&#xa;     count&#x28;distinct a1.id&#x29; active_ads&#xd;&#xa;   from ads a1&#xd;&#xa;     join rdl_triton_livesync.cee_olxpl_categories cat&#xd;&#xa;          on a1.category_id&#x3d;cat.id&#xd;&#xa;     join users a2&#xd;&#xa;       on a1.user_id&#x3d;a2.user_id&#xd;&#xa;   where status&#x3d;&#x27;active&#x27;&#xd;&#xa;   group by a1.user_id,cat.name_pl&#xd;&#xa;&#x29;,&#xd;&#xa;-- active ads , checked&#xd;&#xa;vas1m as -- number of vas bought within last 1 month&#xd;&#xa;&#x28;   select&#xd;&#xa;      a1.id_user,&#xd;&#xa;      cat.name_pl,&#xd;&#xa;      count&#x28;&#x2a;&#x29; vas1m&#xd;&#xa;    from rdl_triton_livesync.cee_olxpl_paidads_user_payments a1&#xd;&#xa;      join users a2&#xd;&#xa;        on a1.id_user&#x3d;a2.user_id&#xd;&#xa;      join ads a3&#xd;&#xa;        on a1.id_ad&#x3d;a3.id&#xd;&#xa;      join rdl_triton_livesync.cee_olxpl_categories cat&#xd;&#xa;        on a3.category_id&#x3d;cat.id&#xd;&#xa;   where a1.livesync_dbname&#x3d;&#x27;olxpl&#x27;&#xd;&#xa;     and a1.id_index in &#x28;3,4,99,101&#x29;&#xd;&#xa;     and a1.price&#x3c;0&#xd;&#xa;     and a1.date &#x3e;&#x3d; CURRENT_DATE&#xd;&#xa;group by a1.id_user,cat.name_pl&#xd;&#xa;&#x29;,&#xd;&#xa;vas6m as -- number of vas bought within last 6 months&#xd;&#xa;&#x28;   select&#xd;&#xa;      a1.id_user,&#xd;&#xa;      cat.name_pl,&#xd;&#xa;      count&#x28;&#x2a;&#x29; vas6m&#xd;&#xa;    from rdl_triton_livesync.cee_olxpl_paidads_user_payments a1&#xd;&#xa;      join users a2&#xd;&#xa;        on a1.id_user&#x3d;a2.user_id&#xd;&#xa;      join ads a3&#xd;&#xa;        on a1.id_ad&#x3d;a3.id&#xd;&#xa;      join rdl_triton_livesync.cee_olxpl_categories cat&#xd;&#xa;        on a3.category_id&#x3d;cat.id&#xd;&#xa;where     a1.livesync_dbname&#x3d;&#x27;olxpl&#x27;&#xd;&#xa;      and a1.id_index in &#x28;3,4,99,101&#x29;&#xd;&#xa;      and a1.price&#x3c;0&#xd;&#xa;      and trunc&#x28;a1.date&#x29;&#x3e;&#x3d; CURRENT_DATE-180&#xd;&#xa;group by a1.id_user, cat.name_pl&#xd;&#xa;&#x29;,&#xd;&#xa;reve1m as -- revenue gross last month&#xd;&#xa;&#x28;     select&#xd;&#xa;        a1.id_user,&#xd;&#xa;        cat.name_pl,&#xd;&#xa;        sum&#x28;a1.price&#x29;&#x2a;-1 reve1m&#xd;&#xa;      from rdl_triton_livesync.cee_olxpl_paidads_user_payments a1&#xd;&#xa;        join users a2&#xd;&#xa;          on a1.id_user&#x3d;a2.user_id&#xd;&#xa;        join ads a3&#xd;&#xa;          on a1.id_ad&#x3d;a3.id&#xd;&#xa;        join rdl_triton_livesync.cee_olxpl_categories cat&#xd;&#xa;          on a3.category_id&#x3d;cat.id&#xd;&#xa;      where&#xd;&#xa;        a1.livesync_dbname&#x3d;&#x27;olxpl&#x27;&#xd;&#xa;        and a1.id_index in &#x28;3,4,99,101&#x29;&#xd;&#xa;        and a1.price&#x3c;0&#xd;&#xa;        and trunc&#x28;a1.date&#x29;&#x3e;&#x3d; CURRENT_DATE&#xd;&#xa;group by a1.id_user, cat.name_pl&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;reve6m as -- revenue gross last 6 months&#xd;&#xa;&#x28;   select a1.id_user,&#xd;&#xa;      cat.name_pl,&#xd;&#xa;      sum&#x28;a1.price&#x29;&#x2a;-1 reve6m&#xd;&#xa;    from rdl_triton_livesync.cee_olxpl_paidads_user_payments a1&#xd;&#xa;      join users a2&#xd;&#xa;        on a1.id_user&#x3d;a2.user_id&#xd;&#xa;      join ads  a3&#xd;&#xa;        on a1.id_ad&#x3d;a3.id&#xd;&#xa;      join rdl_triton_livesync.cee_olxpl_categories cat&#xd;&#xa;          on a3.category_id&#x3d;cat.id&#xd;&#xa;     where a1.livesync_dbname&#x3d;&#x27;olxpl&#x27;&#xd;&#xa;     and a1.id_index in &#x28;3,4,99,101&#x29;&#xd;&#xa;     and a1.price&#x3c;0&#xd;&#xa;     and trunc&#x28;a1.date&#x29; &#x3e;&#x3d; CURRENT_DATE-180&#xd;&#xa;group by a1.id_user, cat.name_pl&#xd;&#xa;&#x29;,&#xd;&#xa;usrs as &#x28;&#xd;&#xa;  SELECT a1.user_id, a1.email, a1.name_pl,&#xd;&#xa;    coalesce&#x28;a2.nnls, 0&#x29; nnls,&#xd;&#xa;    coalesce&#x28;a3.active_ads, 0&#x29; active_ads,&#xd;&#xa;    coalesce&#x28;a4.vas1m, 0&#x29; vas1m,&#xd;&#xa;    coalesce&#x28;a5.vas6m,0&#x29; vas6m,&#xd;&#xa;    coalesce&#x28;reve1m, 0&#x29; reve1m,&#xd;&#xa;    coalesce&#x28;reve6m, 0&#x29; reve6m&#xd;&#xa;  from users a1&#xd;&#xa;  left join nnls a2 on a1.user_id&#x3d;a2.user_id and a1.name_pl&#x3d;a2.name_pl&#xd;&#xa;  left join active_ads a3 on a1.user_id&#x3d;a3.user_id and a1.name_pl&#x3d;a3.name_pl&#xd;&#xa;  left join vas1m a4 on a1.user_id&#x3d;a4.id_user and a1.name_pl&#x3d;a4.name_pl&#xd;&#xa;  left join vas6m a5 on a1.user_id&#x3d;a5.id_user and a1.name_pl&#x3d;a5.name_pl&#xd;&#xa;  left join reve1m a6 on a1.user_id&#x3d;a6.id_user and a1.name_pl&#x3d;a6.name_pl&#xd;&#xa;  left join reve6m a7 on a1.user_id&#x3d;a7.id_user and a1.name_pl&#x3d;a7.name_pl&#x29;,&#xd;&#xa;bckts as &#x28;&#xd;&#xa;  select u.user_id,&#xd;&#xa;    case when sum&#x28;active_ads&#x29;&#x3e;1 and sum&#x28;vas1m&#x29;&#x3e;&#x3d;1 then &#x27;GOLD&#x27;&#xd;&#xa;      when sum&#x28;active_ads&#x29;&#x3d;1 and sum&#x28;vas1m&#x29;&#x3e;&#x3d;1 then &#x27;SILVER&#x27;&#xd;&#xa;      when &#x28;sum&#x28;active_ads&#x29;&#x3e;&#x3d;1 and sum&#x28;vas1m&#x29;&#x3d;0 and sum&#x28;vas6m&#x29;&#x3e;&#x3d;1&#x29; OR &#x28;sum&#x28;active_ads&#x29;&#x3d;0 and  sum&#x28;vas6m&#x29;&#x3e;&#x3d;1&#x29; then &#x27;BRONZE&#x27;&#xd;&#xa;      when sum&#x28;active_ads&#x29;&#x3e;&#x3d;1 and sum&#x28;vas6m&#x29;&#x3d;0 then &#x27;TIN&#x27;&#xd;&#xa;      when sum&#x28;active_ads&#x29;&#x3d;0 and sum&#x28;vas6m&#x29;&#x3d;0 then &#x27;DROP-OFF&#x27;&#xd;&#xa;    end as bucket&#xd;&#xa;  from usrs u&#xd;&#xa;  group by u.user_id&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;select u.&#x2a;, b.bucket, CURRENT_DATE-180 as start_date, CURRENT_DATE as end_date,  CURRENT_DATE as inserted_date&#xd;&#xa;  from bckts b&#xd;&#xa;    join usrs u&#xd;&#xa;      on b.user_id &#x3d; u.user_id&#x3b;</sql>
