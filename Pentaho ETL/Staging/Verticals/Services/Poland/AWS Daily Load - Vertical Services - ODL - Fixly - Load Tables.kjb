    <entry>
      <name>log and delta - segmentation process</name>
      <description/>
      <type>SQL</type>
      <sql>&#xd;&#xa;--LOG&#xd;&#xa;--populate log table&#xd;&#xa;-- delete the current day from log&#xd;&#xa;delete from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- insert date &#x3a; process&#xd;&#xa;insert into rdl_vertical_services.fixly_bi_control_segmentation_log &#x28;date_process&#x29; values &#x28;current_date&#x29;&#x3b;&#xd;&#xa;-- insert total rows&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log set qty_total_rows&#x3d;&#xd;&#xa;&#x28;select count&#x28;&#x2a;&#x29; from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- insert qty distinct users id&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log set qty_user_id&#x3d;&#xd;&#xa;&#x28;select count&#x28;distinct user_id&#x29; from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- insert qty distinct email&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log set qty_email&#x3d;&#xd;&#xa;&#x28;select count&#x28;distinct email&#x29; from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- qty gold&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log set qty_gold&#x3d;&#xd;&#xa;&#x28;select count&#x28;case when bucket &#x3d; &#x27;GOLD&#x27; then 1 end&#x29; as qty_gold from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- qty silver&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log&#xd;&#xa;set qty_silver&#x3d;&#xd;&#xa;   &#x28;select count&#x28;case when bucket &#x3d; &#x27;SILVER&#x27; then 1 end&#x29; as qty_silver from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- qty bronze&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log&#xd;&#xa;set qty_bronze&#x3d;&#xd;&#xa;   &#x28;select count&#x28;case when bucket &#x3d; &#x27;BRONZE&#x27; then 1 end&#x29; as qty_bronze from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- qty tin&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log&#xd;&#xa;set qty_tin&#x3d;&#xd;&#xa;   &#x28;select count&#x28;case when bucket &#x3d; &#x27;TIN&#x27; then 1 end&#x29; as qty_tin from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- select &#x2a; from rdl_vertical_services.fixly_bi_control_segmentation_log&#x3b;&#xd;&#xa;-- check point&#xd;&#xa;-- qty drop off users&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log&#xd;&#xa;set qty_dropoff&#x3d;&#xd;&#xa;   &#x28;select count&#x28;case when bucket &#x3d; &#x27;DROP-OFF&#x27; then 1 end&#x29; as qty_dropoff from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- porc users with email&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log&#xd;&#xa;set porc_user_with_email&#x3d;&#xd;&#xa;&#x28;select count&#x28;distinct email&#x29;&#x3a;&#x3a;FLOAT&#x2f;count&#x28;distinct user_id&#x29;&#x3a;&#x3a;FLOAT &#x2a; 100  from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- porc users without email&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log&#xd;&#xa;set porc_user_without_email&#x3d;&#xd;&#xa;&#x28;select 100-&#x28;count&#x28;distinct email&#x29;&#x3a;&#x3a;FLOAT&#x2f;count&#x28;distinct user_id&#x29;&#x3a;&#x3a;FLOAT &#x2a; 100&#x29;  from odl_global_verticals.vert_services_fixly_buckets_segment&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--DELTA&#xd;&#xa;--populate porc delta &#x3a; yesterday and today running. &#x3a;&#x3a; daily process table&#xd;&#xa;-- delete the current day from log&#xd;&#xa;delete from rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- insert date &#x3a; process&#xd;&#xa;insert into rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun &#x28;date_process&#x29; values &#x28;current_date&#x29;&#x3b;&#xd;&#xa;-- insert total rows&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_total_rows&#x3d;&#xd;&#xa;  &#x28;&#x28;select qty_total_rows from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select qty_total_rows from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- insert qty distinct users id&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_user_id&#x3d;&#xd;&#xa;  &#x28;&#x28;select qty_user_id from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select qty_user_id from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- insert qty distinct email&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_email&#x3d;&#xd;&#xa;  &#x28;&#x28;select qty_email from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select qty_email from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- qty gold&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_gold&#x3d;&#xd;&#xa;  &#x28;&#x28;select qty_gold from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select qty_gold from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- qty silver&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_silver&#x3d;&#xd;&#xa;  &#x28;&#x28;select qty_silver from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select qty_silver from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- qty bronze&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_bronze&#x3d;&#xd;&#xa;  &#x28;&#x28;select qty_bronze from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select qty_bronze from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- qty tin&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_tin&#x3d;&#xd;&#xa;  &#x28;&#x28;select qty_tin from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select qty_tin from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- qty drop off users DELTA&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_dropoff&#x3d;&#xd;&#xa;  &#x28;&#x28;select qty_dropoff from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select qty_dropoff from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- select &#x2a; from rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun&#x3b;&#xd;&#xa;-- porc users with email delta&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_user_with_email_delta&#x3d;&#xd;&#xa;  &#x28;&#x28;select porc_user_with_email from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select porc_user_with_email from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- porc users with email current run&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_user_with_email_onlylastrun&#x3d;&#xd;&#xa;   &#x28;select porc_user_with_email from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- porc users without email delta&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_user_without_email_delta&#x3d;&#xd;&#xa;  &#x28;&#x28;select porc_user_without_email from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#x3a;&#x3a;float &#x2a; 100 &#x2f;&#xd;&#xa;   &#x28;select porc_user_without_email from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date-1&#x29;&#x3a;&#x3a;float&#xd;&#xa;  &#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;-- porc users without email current run&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun set porc_user_without_email_onlylastrun&#x3d;&#xd;&#xa;   &#x28;select porc_user_without_email from rdl_vertical_services.fixly_bi_control_segmentation_log where date_process&#x3d;current_date&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- column RUN_OK --&#x3e; I leave this fix, and then I run the updates to check if is not ok. -- after the porcentajes&#xd;&#xa;drop table if exists temporal_solution&#x3b;&#xd;&#xa;select current_date as today,&#xd;&#xa;  case when   b.porc_total_rows      &#x3c; a.porc_total_rows_allowed then &#x27;no&#x27;&#xd;&#xa;  when        b.porc_user_id         &#x3c; a.porc_user_id_allowed  then &#x27;no&#x27;&#xd;&#xa;  when        b.porc_email           &#x3c; a.porc_email_allowed then &#x27;no&#x27;&#xd;&#xa;  when        b.porc_gold            &#x3c; a.porc_gold_allowed then &#x27;no&#x27;&#xd;&#xa;  when        b.porc_silver          &#x3c; a.porc_silver_allowed then &#x27;no&#x27;&#xd;&#xa;  when        b.porc_bronze          &#x3c; a.porc_bronze_allowed then &#x27;no&#x27;&#xd;&#xa;  when        b.porc_dropoff         &#x3c; a.porc_dropoff_allowed then &#x27;no&#x27;&#xd;&#xa;  when        b.porc_tin             &#x3c; a.porc_tin_allowed  then &#x27;no&#x27;&#xd;&#xa;  when        b.porc_user_with_email_delta &#x3c; a.porc_user_with_email_delta_allowed  then &#x27;no&#x27;&#xd;&#xa;  else &#x27;yes&#x27; end as status&#xd;&#xa;into temporal_solution&#xd;&#xa;  from&#xd;&#xa;  rdl_vertical_services.fixly_bi_control_segmentation_allowed a        cross join&#xd;&#xa;  rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun b&#xd;&#xa;where b.date_process&#x3d;current_date&#xd;&#xa;group by 1,2&#x3b;&#xd;&#xa;&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_delta_lastrun&#xd;&#xa;    set run_ok&#x3d;&#x28;select status from temporal_solution group by 1&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;&#xd;&#xa;update rdl_vertical_services.fixly_bi_control_segmentation_log&#xd;&#xa;    set run_ok&#x3d;&#x28;select status from temporal_solution group by 1&#x29;&#xd;&#xa;where date_process&#x3d;current_date&#x3b;&#xd;&#xa;</sql>
