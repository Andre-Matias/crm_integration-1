    <sql>select product as has_product,count&#x28;&#x2a;&#x29;,ratio_to_report&#x28;count&#x28;&#x2a;&#x29;&#x29; over &#x28;partition by 1&#x29; from &#xa;&#x28;select id ,case qty_products when 0 then FALSE else TRUE end as product from &#xa;&#x28;&#xa;SELECT&#xa;	deals.id, coalesce&#x28;sum&#x28;products.quantity&#x29;,0&#x29; qty_products &#xa;FROM&#xa;	&#x28;select a.id,a.updated_at,a.stage_id from &#xa;			rdl_basecrm.stg_d_base_deals_200_latam a&#xa;			inner join&#xa;				&#x28;select id,max&#x28;inserted_date&#x29; as inserted_date from rdl_basecrm.stg_d_base_deals_200_latam group by 1&#x29; b&#xa;			on &#x28;a.inserted_date&#x3d;b.inserted_date and a.id&#x3d;b.id&#x29; &#xa;			where last_stage_change_at between &#x27;&#x24;&#x7b;DATE&#x7d;&#x27; and DATEADD&#x28;day,6,&#x27;&#x24;&#x7b;DATE&#x7d;&#x27;&#x29;&#xa;			&#x29; deals&#xa;inner join &#xa;		rdl_basecrm.str_d_base_stages_200_latam stages&#xa;on deals.stage_id &#x3d; stages.id&#xa;left join&#xa;	rdl_basecrm.stg_d_base_line_items_200_latam products&#xa;on deals.id &#x3d; products.deal_id	&#xa;where &#xa;stages.name in &#x28;&#x27;Pre-Venta&#x27;,&#x27;Venta Conciliada&#x27;,&#x27;Activo&#x27;&#x29;&#xa;group by deals.id&#xa;&#x29;&#xa;&#x29;group by 1</sql>
