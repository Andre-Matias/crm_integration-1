    <sql>SELECT &#x27;&#x24;&#x7b;DATE_FROM&#x7d;&#x27; as date,&#x27;&#x24;&#x7b;AGG&#x7d;&#x27; as month,A.country_id,&#x27;&#x24;&#x7b;CATEGORY_ID&#x7d;&#x27; as category_id, count&#x28;distinct&#x28;B.user_id&#x29;&#x29; AS PPU&#xa;FROM latam_sandbox.latam_ft_d_items_with_paid_products AS B, &#x28;&#xa;SELECT&#xa;A.country_id,&#xa;A.user_id,&#xa;A.time_id&#xa;FROM ods_naspers.ft_h_listing AS A&#xa;WHERE A.country_id IN &#x28;32, 170, 218, 604&#x29;&#xa;AND A.category_l2_id &#x3d; 378&#xa;AND A.device_source_id &#x3d; 27&#xa;AND A.platform_id &#x3d; 1&#xa;AND A.live_id &#x3d; 1&#xa;AND A.time_id BETWEEN &#x27;&#x24;&#x7b;DATE_FROM&#x7d;&#x27; AND &#x27;&#x24;&#x7b;DATE_TO&#x7d;&#x27;&#xa;&#x29; AS A&#xa;WHERE A.user_id &#x3d; B.user_id&#xa;AND A.country_id &#x3d; B.country_id&#xa;AND B.featured_time_id BETWEEN &#x27;&#x24;&#x7b;DATE_FROM&#x7d;&#x27; AND &#x27;&#x24;&#x7b;DATE_TO&#x7d;&#x27;&#xa;AND B.product_id IN &#x28;9,10,1,2,3,7,8&#x29;&#xa;GROUP BY 1,2,3&#xa;ORDER BY 1,2,3</sql>
