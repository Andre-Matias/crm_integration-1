    <sql>select&#xa;  date_trunc&#x28;&#x27;week&#x27;, dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x29;&#x3a;&#x3a;date as first_day_of_week,&#xa;  to_char&#x28;date_trunc&#x28;&#x27;week&#x27;, dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x29;&#x3a;&#x3a;date,&#x27;YYYY&#x2f;MM&#x2f;DD&#x27;&#x29; as week_path,&#xa;  extract&#x28;week from dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x29; as week_number,&#xa;  date_trunc&#x28;&#x27;month&#x27;, dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x29;&#x3a;&#x3a;date as first_day_of_month,&#xa;  to_char&#x28;date_trunc&#x28;&#x27;month&#x27;, dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x29;&#x3a;&#x3a;date,&#x27;YYYY&#x2f;MM&#x2f;DD&#x27;&#x29; as month_path,&#xa;  to_char&#x28;dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;,&#x27;YYYY-MM&#x27;&#x29; as month,&#xa;  to_char&#x28;dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;,&#x27;MON&#x27;&#x29; as month_name,&#xa;  dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x3a;&#x3a;date as yesterday,&#xa;  to_char&#x28;dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x3a;&#x3a;date,&#x27;YYYY&#x2f;MM&#x2f;DD&#x27;&#x29; as yesterday_path,&#xa;  dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29; as updated_at,&#xa;  dateadd&#x28;month,-1,date_trunc&#x28;&#x27;month&#x27;, dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x29;&#x29;&#x3a;&#x3a;date as first_day_of_previous_month,&#xa;  to_char&#x28;dateadd&#x28;day,-1,&#x28;date_trunc&#x28;&#x27;month&#x27;, dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x29;&#x3a;&#x3a;date&#x29;&#x29;,&#x27;YYYY-MM-DD&#x27;&#x29; as last_day_of_previous_month,&#xa;  to_char&#x28;dateadd&#x28;month,-1,date_trunc&#x28;&#x27;month&#x27;, dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x29;&#x29;&#x3a;&#x3a;date,&#x27;YYYY&#x2f;MM&#x2f;DD&#x27;&#x29; as previous_month_path,&#xa;  to_char&#x28;dateadd&#x28;month,-1,date_trunc&#x28;&#x27;month&#x27;, dateadd&#x28;day,-1,&#x27;2017-12-28&#x27;&#x29;&#x29;&#x29;&#x3a;&