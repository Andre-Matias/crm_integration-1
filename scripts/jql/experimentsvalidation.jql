 return Events({
   from_date: '2017-10-01',
   to_date:   '2017-11-01'
 })
  .filter(function(e) {
    return (
      e.name.includes('draft') === true 
    || e.name == ('posting_form')
    || e.name.includes('posting') === true
    )
    && 
    (
      e.properties.experiments ==  '9032360167:9024060291'
      || e.properties.experiments ==  '9032360167:9010870766'
      )
    ;
 })
 .groupBy(['name', 'properties.experiments'], mixpanel.reducer.count());
}
